  "version": "0.2.0",
  "configurations": [
    {
      "name": "tests",
      "type": "cppdbg",
      "request": "launch",
      "program": "/home/ben/bin/node/bin/node",
      "args": ["./test/test_binding.js"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "CTS",
      "type": "cppdbg",
      "request": "launch",
      "program": "/home/ben/bin/node/bin/node",
      "args": ["./test.js", "--verbose", "webgpu:api,*"],
      "stopAtEntry": false,
      "cwd": "/home/ben/src/gpuweb-cts",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "CTS (debug node)",
      "type": "cppdbg",
      "request": "launch",
      "program": "/home/ben/src/node/out/Debug/node",
      "args": ["./test.js", "--verbose", "webgpu:api,*"],
      "stopAtEntry": false,
      "cwd": "/home/ben/src/gpuweb-cts",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
